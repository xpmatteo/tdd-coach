# API Configurations
# You only need the API key for OpenRouter now.
OPENROUTER_API_KEY=

# OpenRouter Model Selection
# Examples: "anthropic/claude-3-7-sonnet", "openai/gpt-4o", etc.
# This model will be used for AI coaching.
OPENROUTER_MODEL=anthropic/claude-3-7-sonnet

# Server Configuration
PORT=3000

# Prompt Testing System Configuration (Optional)
# Set to true to enable capturing prompts and LLM responses for testing
PROMPT_CAPTURE_MODE=false
PROMPT_TEST_DIR=./prompt_tests